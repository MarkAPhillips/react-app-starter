{
  "name": "react-app-starter",
  "version": "0.0.1",
  "description": "React app starter using webpack, redux and styled components with jest",
  "main": "src/index.js",
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "build": "webpack --progress --colors --mode production",
    "start": "webpack-dev-server --host 0.0.0.0 --mode=development --history-api-fallback",
    "test": "jest --no-cache",
    "update-snapshots": "jest --updateSnapshot",
    "test:dev": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkAPhillips/react-app-starter.git"
  },
  "author": "Mark Phillips",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarkAPhillips/react-app-starter/issues"
  },
  "homepage": "https://github.com/MarkAPhillips/react-app-starter#readme",
  "jest": {
    "verbose": true,
    "automock": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/config.js"
    ],
    "testRegex": "\\.spec\\.js$",
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "acorn": "^6.1.1",
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.5.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-apollo": "^2.5.2",
    "react-dom": "^16.8.4",
    "react-router": "^4.3.1",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-regenerator": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.4.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.4.0",
    "jest-enzyme": "^7.0.2",
    "jest-styled-components": "^6.3.1",
    "jest-transform-graphql": "^2.1.0",
    "react-test-renderer": "^16.8.4",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
